package doubledeltas.server;

import java.sql.*;

public class Main {
    public static void main(String[] args)
    {
        MySQLConnector con = new MySQLConnector("localhost", "root", "smartist2!");
        if (conn != null) {
            System.out.printf("DB connection success");
        }
        else {
            System.out.printf("DB connection failed");
        }
    }
}
